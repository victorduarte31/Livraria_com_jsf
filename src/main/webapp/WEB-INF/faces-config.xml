<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<application>
		<resource-bundle>
			<base-name>messages.titulos</base-name>
			<var>msgs</var>
		</resource-bundle>
		<message-bundle>messages.titulos_pt_BR</message-bundle>
		<message-bundle>messages.titulos_en_US</message-bundle>
		<locale-config>
			<default-locale>pt_BR</default-locale>
			<supported-locale>en_US</supported-locale>
			<supported-locale>zh_CN</supported-locale>
		</locale-config>
	</application>

	<lifecycle>
		<phase-listener>br.victor.livraria.util.Autorizador</phase-listener>
		<phase-listener>br.victor.livraria.util.LogPhaseListener</phase-listener>
	</lifecycle>

</faces-config>
